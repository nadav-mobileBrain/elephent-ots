{
  "expo": {
    "name": "elephant-mobile",
    "slug": "elephant-mobile",
    "version": "1.4.0",
    "runtimeVersion": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "elephant-mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.elephant-mobile.mccrudy",
      "buildType": "release",
      "prebuildCommand": "npm install --legacy-peer-deps",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "We need your location to show you where you are on the map and to accurately place your elephant sighting pins."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2a60e082-d541-4db8-ab42-a5d8eb753d1e"
      }
    },
    "android": {
      "package": "com.elephantmobile.mccrudy"
    }
  }
}
